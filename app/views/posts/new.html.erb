<h2>Upload a picture now!</h2>
<hr>
<%= form_for @post, html: {multipart: true} do |f| %>
    <div class="image_upload">
    <%= f.label :image %>
    <%= f.file_field :image %>
    </div>
    <br>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <br>
    <%= f.submit %>
<% end %>
